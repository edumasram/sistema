<h1>Listing custo_produtos</h1>

<table>
  <tr>
    <th>Custo</th>
    <th>Produto</th>
    <th>Data</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @custo_produtos.each do |custo_produto| %>
  <tr>
    <td><%= custo_produto.custo %></td>
    <td><%= custo_produto.produto_id %></td>
    <td><%= custo_produto.data %></td>
    <td><%= link_to 'Show', custo_produto %></td>
    <td><%= link_to 'Edit', edit_custo_produto_path(custo_produto) %></td>
    <td><%= link_to 'Destroy', custo_produto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

